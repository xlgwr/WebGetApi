{
    "content_scripts": [
        {
            "matches": [
                "http://www.icris.cr.gov.hk/*",
                "https://www.icris.cr.gov.hk/*",
                "http://*.judiciary.gov.hk/*",
                "https://192.168.1.6:7443/*",
                "http://192.168.1.6:7070/*"
            ],
            "all_frames": true,
            "js": [
                "lib/jquery.min.js",
                "config.js",
                "model/backMsg.js",
                "model/baseFun.js",
                "model/0for_icris.js",
                "model/1for_judiciary.js",
                "model/2for_legalref.js",
                "getwebData.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "icons/icon_32.png",
        //"default_popup": "popup.html",
        "default_title": "Get Web Data"
    },
    "description": "Get Web Data :-)",
    //"key": "",
    "name": "Get Web Data for Chrome",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "64": "icons/icon_64.png"
    },
    "homepage_url": "http://www.baidu.com/",
    "permissions": [
        "tabs",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "update_url": "http://www.baidu.com/",
    "background": {
        "persistent": true,
        "page": "background.html"
        // "scripts": [
        //     "background.js"
        // ]
    }
}